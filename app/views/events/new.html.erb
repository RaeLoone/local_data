<%= render 'map' %>

<html>
<head>
    <title></title>
    <style type="text/css">

    .container {
        width: 600px;
        clear: both;
    }
    .container input {
        width: 70%;
        clear: both;
        
    }
     .container {
     position:absolute;
     z-index:15;
     top:50%;
     left:29.5%;
     margin:-100px 0 0 -150px;
    }
  
    .container form { 
        margin: 100 auto; 
    }
    .container {
        background-color:#c3dfef;
    }
    .container {
        float:right;
        padding:25px;
    }
    
    .container {
        margin: 0;
        padding: 5px 15px;
        font-family: Arial, Helvetica, sans-serif;
        font-size:50px;
        outline: none;
        cursor: pointer;
        text-align: center;
        text-decoration: none;
        color: #ffffff;
        border: solid 0px #0076a3; border-right:0px;
        background: #0095cd;
        background: -webkit-gradient(linear, left top, left bottom, from(#00adee), to(#0078a5));
        background: -moz-linear-gradient(top,  #00adee,  #0078a5);
        border-top-right-radius: 5px 5px;
        border-bottom-right-radius: 5px 5px;
    }
    .container:hover {
        text-decoration: none;
        background: #007ead;
        background: -webkit-gradient(linear, left top, left bottom, from(#0095cc), to(#00678e));
        background: -moz-linear-gradient(top,  #0095cc,  #00678e);
    }
    /* Fixes submit button height problem in Firefox */
    .container::-moz-focus-inner {
      border: 0;
    }
    .container {
        clear:both;
    }
    </style>
    <style type ="text/css">


     .image {
     position:relative;
     z-index:15;
     top:100%;
     left:35%;
     
     height: 200px;
     width: 50%;
     margin-left: auto;
     margin-right: 60.5%;
     margin-top: 12%;
    }


    </style>
</head>
<body>
<div class="image">
<img src="/images/Loogle.jpg" title="Loogle" style="padding-top: 10px ;">
</div>    

<div class="container">
	<%= render 'form' %>
</div>
</body>
</html>
